{
	"name": "@essex-js-toolkit/dev",
	"version": "0.0.1",
	"description": "A set of packages for developers",
	"repository": "git@ssh.dev.azure.com:v3/msrp-essex/Alpha/alpha-developer",
	"author": "David Tittsworth <datittsw@microsoft.com>",
	"contributors": [
		"David Tittsworth <datittsw@microsoft.com>",
		"Nathan Evans <naevans@microsoft.com>",
		"Chris Trevino <chtrevin@microsoft.com>",
		"Amber Hoak <amhoak@microsoft.com>",
		"Eric Kimbrel <lekimbrel@gmail.com>"
	],
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">= 12.0"
	},
	"scripts": {
		"preinstall": "npx only-allow yarn",
		"clean_all": "yarn workspaces foreach -pv run clean",
		"build_all": "yarn workspaces foreach -pvt run build",
		"test_all": "yarn workspaces foreach -pv run test",
		"start_all": "yarn workspaces foreach -pv run start",
		"unit_test": "essex test --coverage",
		"lint": "essex lint --docs",
		"lint:fix": "essex lint -f",
		"git_is_clean": "essex git-is-clean",
		"prettify": "essex prettify",
		"release": "npm run build && lerna publish",
		"scripts:upgrade": "essex install",
		"ci": "run-s build_all lint test_all unit_test git_is_clean",
		"publish": "yarn workspaces foreach --exclude '@essex-build/test-suite-*' --exclude '@essex/alpha-build-utils' -pv npm publish --tolerate-republish --access public"
	},
	"devDependencies": {
		"@essex/eslint-config": "^10.0.0",
		"@essex/eslint-plugin": "^10.0.0",
		"@essex/prettier-config": "^10.0.0",
		"@essex/scripts": "^10.0.1",
		"@types/node": "^14.11.8",
		"debug": "^4.3.0",
		"husky": "^4.3.0",
		"lerna": "^3.22.1",
		"lint-staged": "^10.4.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^4.0.3"
	},
	"workspaces": [
		"packages/*"
	],
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^4.4.1"
	},
	"resolutions": {
		"handlebars": "^4.4.3"
	}
}
