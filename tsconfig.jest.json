{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"target": "es6",
		"declaration": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"downlevelIteration": true,
		"noImplicitAny": false,
		"allowJs": true,
		"strict": false,
		"resolveJsonModule": true
	},
	"include": ["src/**/__tests__/*.spec.tsx?"]
}
