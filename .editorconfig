# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# 4 space indentation
[*.{js,ts,tsx,jsx}]
indent_style = tab
indent_size = 1
tab_width=2

# Tab indentation (no size specified)
[Makefile]
indent_style = tab
